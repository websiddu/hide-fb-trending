{
  "name": "__MSG_appName__",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "tabs",
    "*://*.facebook.com/*",
    "*://*.fb.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*",
        "*://*.fb.com/*"
      ],
      "js": [],
      "css": ["style.css"],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
